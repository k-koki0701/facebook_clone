<h1>ブログ一覧</h1>
<p><%= notice %></p>
<% @blogs.each do |blog| %>
  <tr>
    <td><%= blog.content %></td>
  </tr>
  <%= link_to '詳細', blog_path(blog.id) %>
  <%= link_to "編集", edit_blog_path(blog.id) %>
  <%= link_to "削除", blog_path(blog.id), method: :delete, data:{confirm: '本当に削除しますか？'} %>
<% end %>
</table>
<br>
<%= link_to '新しくブログを投稿する', new_blog_path %>
